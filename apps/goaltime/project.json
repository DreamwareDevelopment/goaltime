{
  "name": "goaltime",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/goaltime",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "dependsOn": ["shared:migrate"]
    },
    "local-proxy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["ngrok start --all"]
      }
    },
    "local-inngest": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx inngest-cli@latest dev"]
      }
    },
    "dev": {
      "dependsOn": ["shared:type-gen"],
      "options": {
        "command": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
        "proxyConfig": "apps/goaltime/proxy.conf.json"
      }
    }
  }
}
